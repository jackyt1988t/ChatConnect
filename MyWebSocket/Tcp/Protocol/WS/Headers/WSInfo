
namespace MyWebSocket.Tcp.Protocol.WS
{
	class WSInfo
	{
		public bool
		    IsFragment
		    {
			get
			{
				if (Fin == WSFin.Next)
					return true;
				else
					return false;
			}
		    }
		public WSFin Fin;
		public WSOpcpd Opcod;
		public WSOpcod LastOpcod;
		public DateTime StartTime;
	}
}
